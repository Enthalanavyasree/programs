class Solution(object):
    def maxSubArray(self,nums):
        """
        :type nums:List[int]
        :rtype: int
        """ 
        n=len(nums)
        cs=0
        ms=float("-inf")
        for i in nums:
            cs +=i
            ms=max(ms, cs)
            if(cs <0):
               cs=0
        return ms
        
